<h1>Workout</h1>

<ul class="workout-card">
    <li class="exercise">Exercise:</li>
    <li class="description">Description:</li>
    <li class="sets">Sets:</li>
    <li class="reps">Reps:</li>
</ul>
<div id="workout-card">
    <select id="dropdown"></select>
    <div id="description">
        <section class="exercise-spec">
            
        </section>
    </div>
</div>

<h1>Workout Data</h1>

<ul>
  {{#each workout}}
  <li>
    <h2>{{name}}</h2>
    <p>{{description}}</p>
  </li>
  {{/each}}
</ul>



<script src="/js/workout.js"></script>














